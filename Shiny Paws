local Tab = Window:CreateTab("Main", 11570802781)
local Section = Tab:CreateSection("Main")
local Button = Tab:CreateButton({
    Name = "Autocollect Gems/Coins/Eggs",
    Callback = function(Val)
        local player = game.Players.LocalPlayer
		local character = player.Character or player.CharacterAdded:Wait()
		local folder = game.Workspace.Orbs

		local function bringAllPartsInFolder()
    	while true do
        for _, part in pairs(folder:GetChildren()) do
            if part:IsA("Part") then
                part.CFrame = character.HumanoidRootPart.CFrame
            end
        end
        wait(0.0001)
    end
end
bringAllPartsInFolder()
    end,
})
