local Button = Tab2:CreateButton({
   Name = "Farm Dummy [Spawn]",
   Callback = function()
   	while true do
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-125.49247741699219, 650.7031860351562, 595.1104125976562)
		while true do
			local args = {
				[1] = workspace:WaitForChild("MAP"):WaitForChild("dummies"):WaitForChild("Dummy"):WaitForChild("Humanoid"),
				[2] = 3
			}

			game:GetService("ReplicatedStorage"):WaitForChild("jdskhfsIIIllliiIIIdchgdIiIIIlIlIli"):FireServer(unpack(args))
			wait()
		end
	end
   end,
})
