-- Scripts:

local function ICSYIUQ_fake_script() -- AnimalSimFree.LocalScript 
	local script = Instance.new('LocalScript', AnimalSimFree)

	local SGui = script.Parent
	local Frame = SGui:WaitForChild("Frame")
	local Button = SGui:WaitForChild("TextButton")
	
	Button.MouseButton1Up:Connect(function()
		Frame.Visible = not Frame.Visible
	end)
end
coroutine.wrap(ICSYIUQ_fake_script)()
